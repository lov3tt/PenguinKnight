  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "concurrently": "^4.1.1",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "express-handlebars": "^3.1.0",
    "express-session": "^1.16.2",
    "materialize-css": "^1.0.0-rc.2",
    "mysql2": "^1.6.5",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^5.15.1",
    "serve": "^6.1.0"
  }
}
